
<h1>Contacts#index</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Admin</th>
      
  
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.admin %></td>
       <td><%= link_to 'Edit', edit_user_path(user.id)%> <td><%= link_to 'show', user %> <td><%= button_to 'destroy', user, :method => :delete, data: {confirm: "Sure?"} %></td></td> 
 
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New user', new_user_path %>
