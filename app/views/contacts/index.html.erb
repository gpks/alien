<h1>Contacts#index</h1>

<table>
  <thead>
    <tr>
      <th>City Id</th>
      <th>Objects</th>
      <th>Number of contacts</th>
      
  
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.city_id %></td>
        <td><%= contact.objects %></td>
       <td><%= link_to 'Edit', edit_contact_path(contact.id)%> <td><%= link_to 'show', contact %> <td><%= button_to 'destroy', contact, :method => :delete, data: {confirm: "Sure?"} %></td></td> 
 
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New contact', new_contact_path %>
