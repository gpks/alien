<%= will_paginate @cities %>
<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Liczba zarejestrowanych kontaktów</th>
      <th>Liczba obiektów</th>
      
      
  
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>

        <td><%= city.name %></td>

        <td><%= city.contacts.count %></td>
        <td><%= city.contacts.sum(:objects) %></td>
        
        
      
      
      
        
       
 
      </tr>
    <% end %>
  </tbody>
</table>


